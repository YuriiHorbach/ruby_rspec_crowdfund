<h1>Edit page</h1>

<%= form_for @project do |f| %>
  <p>
    <%= f.label :name %> <br>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %> <br>
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :target %> <br>
    <%= f.number_field :target %>
  </p>
  <p>
    <%= f.label :release %> <br>
    <%= f.date_field :release %>
  </p>
  <p>
    <%= f.label :website %> <br>
    <%= f.text_field :website %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>